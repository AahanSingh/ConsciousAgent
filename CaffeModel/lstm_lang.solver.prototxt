net: "lstm_lang.prototxt"
test_state: { stage: 'test-on-train' }
test_iter: 25
test_interval: 1000
base_lr: 0.1
lr_policy: "step"
gamma: 0.5
stepsize: 20000
display: 1
max_iter: 110000
momentum: 0.9
weight_decay: 0.0000
snapshot: 5000
snapshot_prefix: "lstm_lang"
solver_mode: GPU
random_seed: 1701
average_loss: 100
clip_gradients: 10