net: "LRCN.prototxt"
test_state: { stage: 'test-on-train' }
test_iter: 25
test_interval: 1000
base_lr: 0.01
lr_policy: "step"
gamma: 0.5
stepsize: 10000
display: 1
max_iter: 50000
momentum: 0.9
weight_decay: 0.0000
snapshot: 1000
snapshot_prefix: "LRCNModel"
solver_mode: GPU
random_seed: 1995
average_loss: 100
clip_gradients: 10
